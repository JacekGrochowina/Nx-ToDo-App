              "libs/type/tsconfig.lib.json",
              "libs/type/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/type/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/type"],
          "options": {
            "jestConfig": "libs/type/jest.config.js",
            "passWithNoTests": true
          }
