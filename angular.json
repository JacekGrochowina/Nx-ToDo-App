            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/todo/todo-list-feature"],
          "options": {
            "jestConfig": "libs/todo/todo-list-feature/jest.config.js",
